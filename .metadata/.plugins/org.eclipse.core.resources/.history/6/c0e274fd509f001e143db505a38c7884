package vijay;

public class AndOR {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a =1;
		int b=2;
		if(a>b && a==b) {
			System.out.println(true);
		}
		else
		{
			System.out.println(false);
		}

		
		//AND
		
		//TRUE FALSE = FALSE
		//TRUE TRUE = TRUE
		//FALSE TRUE=FALSE
		//FALSE FALSE = FALSE
		
		//OR
		
				//TRUE FALSE = TRUE
				//TRUE TRUE = TRUE
				//FALSE TRUE=TRUE
				//FALSE FALSE = FALSE
	}

}

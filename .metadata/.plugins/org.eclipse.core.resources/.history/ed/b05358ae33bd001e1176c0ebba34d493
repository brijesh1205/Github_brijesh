public class Fetch_create_pudate {
 
	
	@Test
	void fetchdata()
	{
		given()
		
		
		.when()
		.get("https://reqres.in/api/users?page=2")
		
		.then()
		.statusCode(200)
		.log().all();
	}
	
	
	
}

		float e=14.5f;
		int f=13;
		
		System.out.println(e>f); //true
		System.out.println(e<f);  //false
		System.out.println(e>=f);  //true  
		System.out.println(e<=f);  //false
		System.out.println(e==f);  //false
		
		
		float g=13.0f;
		int h=13;
		
		System.out.println(g>h); //true
		System.out.println(g<h);  //false
		System.out.println(g>=h);  //true  
		System.out.println(g<=h);  //false
		System.out.println(g==h);  //false

